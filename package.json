{
  "name": "sbvr-vscode",
  "displayName": "SBVR",
  "description": "SBVR standard grammar & syntax highlight.\nThe SBVR standard defines the vocabulary and rules for documenting the semantics of business vocabularies, business facts, and business rules; as well as an XMI schema for the interchange of business vocabularies and business rules among organizations and between software tools.",
  "version": "0.0.1",
  "publisher": "voiceapi",
  "private": true,
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/voiceapiai/sbvr-vscode"
  },
  "engines": {
    "vscode": "^1.76.0"
  },
  "categories": [
    "Other",
    "Language Packs"
  ],
  "capabilities": {},
  "contributes": {
    "languages": [
      {
        "id": "sbvr",
        "aliases": [
          "SBVR",
          "sbvr"
        ],
        "extensions": [
          ".sbvr"
        ],
        "icon": {
          "light": "./icons/roadmap.svg",
          "dark": "./icons/roadmap.svg"
        },
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sbvr",
        "scopeName": "text.sbvr",
        "path": "./syntaxes/sbvr.tmLanguage.json"
      }
    ]
  },
  "activationEvents": [],
  "icon": "./icons/roadmap.png",
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -watch -p ./",
    "deploy": "vsce publish --yarn"
  },
  "dependencies": {
    "@balena/sbvr-parser": "^1.4.3",
    "@balena/sbvr-types": "^3.5.0",
    "vscode-languageclient": "^8.1.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "@vscode/test-electron": "^2.2.3",
    "@vscode/vsce": "^2.18.0",
    "eslint": "^8.26.0",
    "js-yaml": "^4.1.0",
    "prettier": "^2.8.4",
    "typescript": "^4.9.4",
    "vscode-languageserver-textdocument": "^1.0.1"
  }
}
